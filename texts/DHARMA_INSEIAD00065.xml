          <app loc="6">
            <lem>sādh<supplied reason="lost">u</supplied><add place="above">ja</add>nābhinaṁdi –</lem>
            <rdg source="bib:Sircarl">sādhujanābhinaṁdi |<supplied reason="omitted">|</supplied></rdg>
