          <app loc="3">
            <lem>Aimpatāvat<unclear>iṉ karē</unclear></lem>
            <rdg source="bib:Sampath1968_01">Aimpatāvatiṉ karē</rdg>
            <rdg source="bib:Mahalingam1988_01">Aimpatāvat<unclear>iṉ karē</unclear></rdg>
            <rdg source="bib:Dayalan2005_01">Aimpatāvatiṉ karai</rdg>
