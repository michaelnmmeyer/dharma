      <div type="edition" xml:lang="okz-Latn">
        <div n="A" subtype="fragment" type="textpart">
          <p>
   <lb n="A1"/><gap extent="unknown" reason="lost" unit="character"/>
   <lb n="A2"/><gap extent="unknown" reason="lost" unit="character"/> <g type="dotMid">.</g> va tkur· <g type="dotMid">.</g> va <unclear>t</unclear>k<seg subtype="vowel" type="component">
              <gap quantity="1" reason="illegible" unit="component"/>
            </seg> <gap extent="unknown" reason="lost" unit="character"/>
   <lb n="A3"/><gap extent="unknown" reason="lost" unit="character"/> <g type="dotMid">.</g> va ka<seg subtype="body" type="component">
              <gap quantity="1" reason="illegible" unit="component"/>
            </seg><seg subtype="subscript" type="component">
              <gap quantity="1" reason="illegible" unit="component"/>
            </seg>oṅ· <g type="dotMid">.</g> va <unclear>t</unclear><seg subtype="body" type="component">
              <gap quantity="1" reason="illegible" unit="component"/>
            </seg><seg subtype="vowel" type="component">
              <gap quantity="1" reason="illegible" unit="component"/>
            </seg> <gap extent="unknown" reason="lost" unit="character"/>
</p>
        </div>
        <div n="B" subtype="fragment" type="textpart">
          <p>
   <lb n="B1"/><gap extent="unknown" reason="lost" unit="character"/>
   <lb n="B2"/><gap extent="unknown" reason="lost" unit="character"/> <num value="1">I</num> vā <unclear cert="low">j</unclear>mola <gap extent="unknown" reason="lost" unit="character"/>
   <lb n="B3"/><gap extent="unknown" reason="lost" unit="character"/> <unclear>v</unclear>ā k<unclear>v</unclear>ī Ara <gap extent="unknown" reason="lost" unit="character"/>
   <lb n="B4"/><gap extent="unknown" reason="lost" unit="character"/>ḥ <num value="1">I</num> vā <gap extent="unknown" reason="lost" unit="character"/>
</p>
        </div>
      </div>
