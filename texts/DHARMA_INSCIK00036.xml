            </seg>
          </l>
          <l n="b">
            <milestone n="b" unit="column"></milestone>
            <seg met="====-+-=">
              <gap quantity="8" reason="lost" unit="character"></gap>
            </seg>
          </l>
          <l n="c"><milestone n="c" unit="column"></milestone><seg met="=">
              <gap quantity="1" reason="lost" unit="character"></gap>
            </seg> yo 'rccā sthāpitā tena</l>
          <l n="d"><milestone n="d" unit="column"></milestone>bhakty<supplied reason="lost">ā</supplied> <seg met="==-+-=">
              <gap quantity="6" reason="lost" unit="character"></gap>
            </seg> <supplied reason="lost">
              <g type="dDandaOrn">.</g>
            </supplied></l>
        </lg>
        <lg met="anuṣṭubh" n="4">
          <l n="a">
            <lb n="5"></lb>
            <milestone n="a" unit="column"></milestone>
            <seg met="====-++=">
              <gap quantity="8" reason="lost" unit="character"></gap>
            </seg>
          </l>
