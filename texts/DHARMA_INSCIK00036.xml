      <div type="edition" xml:lang="san-Latn">
        <lg met="anuṣṭubh" n="1">
          <l n="a"><lb n="1"></lb><milestone n="a" unit="column"></milestone><seg met="====-++">
              <gap quantity="7" reason="lost" unit="character"></gap>
            </seg>tra</l>
          <l n="b"><milestone n="b" unit="column"></milestone>vāsudevāya vai namaḥ</l>
          <l n="c">
            <lb n="2"></lb>
            <milestone n="a" unit="column"></milestone>
            <seg met="====-++=">
              <gap quantity="8" reason="lost" unit="character"></gap>
            </seg>
          </l>
          <l n="d"><milestone n="b" unit="column"></milestone>dātā tāṁ śaranārthine <g type="dDandaOrn">.</g></l>
        </lg>
        <lg met="anuṣṭubh" n="2">
          <l n="a">
            <lb n="3"></lb>
