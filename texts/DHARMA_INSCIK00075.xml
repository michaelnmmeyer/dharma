        <p><milestone n="A" type="pagelike" unit="face"/>
                        <lb n="A1"/><unclear>
            <g type="circleHorned">.</g>
          </unclear> sabbhamastu <supplied reason="lost">maṅ</supplied><lb break="no" n="A2"/><supplied reason="lost">ga</supplied>lā jaiyyā <supplied reason="lost">tireka</supplied>
                        <lb n="A3"/><num value="990">990</num> sā<gap extent="unknown" reason="lost" unit="character"/>
                     </p>
        <p><milestone n="B" type="pagelike" unit="face"/>
                        <lb n="B1"/>jesa <num value="5">5</num> roja buddhabā<lb break="no" n="B2"/>rasāraṇā mangi saṁtaeca
                        <lb n="B3"/>braḥ varamātā pparammapubi
                     </p>
        <p><milestone n="C" type="pagelike" unit="face"/>
                        <lb break="no" n="C1"/>tra Oya dūla saṁmtaeca braḥ
                        <lb n="C2"/>rājjaputa parammanāṭṭha para<lb break="no" n="C3"/>mapubita draṅa braḥ kūrṇā
                     </p>
        <p><milestone n="D" type="pagelike" unit="face"/>
                        <lb n="D1"/>thā sraeha neḥ Aṁbīya qna<lb break="no" n="D2"/>ka mahāOppārāja bra<lb break="no" n="D3"/>gena qnaka soma Ita niṅa qna
                     </p>
        <p><milestone n="E" type="pagelike" unit="face"/>
                        <lb break="no" n="E1"/>ka soma qnaka saṁtaeca braḥ rājakhsātr<supplied reason="lost">ī</supplied>
                        <lb n="E2"/>bragena qnaka saṁtaeca braḥ bodhīñāṇa
                        <lb n="E3"/>viña srae Oka ñā vaṅsāparā<gap extent="unknown" reason="lost" unit="character"/>
                     </p>
        <p><milestone n="F" type="pagelike" unit="face"/>
                        <lb n="F1"/>qnaka smanavirayā<num value="2">2</num> thvāya qnaka saṁ<lb break="no" n="F2"/>teca braḥ rājakhsātrīya<num value="2">2</num> ma brage<lb break="no" n="F3"/>na qnaka saṁteca braḥ bodhīñā<supplied reason="lost">ṇa</supplied>
                     </p>
        <p><milestone n="G" type="pagelike" unit="face"/>
                        <lb n="G1"/>Ina maka bibāta sratiya debva samteca
                        <lb n="G2"/>bodhīñāna mahāsagharāja thleṅa qna<lb break="no" n="G3"/>ka samtaeca braḥ dā<gap extent="unknown" reason="lost" unit="character"/>ma pubita
                     </p>
