            <ab>

						<lb n="1"/>
						<unclear>na</unclear>mo
					bhagavato
					<space quantity="1" unit="character"/>
					°agapogalasa
					<lb n="2"/>
					raṁño
					gotamiputasa
					<space quantity="1" unit="character"/>
					sirivijayasa<lb n="3" break="no"/>takaṁṇṇisa
					<space quantity="1" unit="character"/>
					sava
					<num value="6">6</num>
					gipa
