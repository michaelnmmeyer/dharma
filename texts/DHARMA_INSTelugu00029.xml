               <lb n="12"/><gap quantity="1" reason="undefined" unit="character"/>bhāṇa<unclear>rā</unclear><lb break="no" n="13"/><gap quantity="1" reason="undefined" unit="character"/> vracci<unclear>na</unclear><lb break="no" n="14"/><gap quantity="1" reason="undefined" unit="character"/>ru pa<gap quantity="1" reason="undefined" unit="character"/><lb break="no" n="15"/><gap quantity="1" reason="undefined" unit="character"/>goṭu<lb break="no" n="16"/><unclear>vā</unclear>ru
