          <rdg source="bib:Boechari1985-1986_01">satuhan</rdg>
        </app>
        <app loc="1v5">
          <lem source="bib:Boechari1985-1986_01">Iṁ satuhān</lem>
          <rdg source="bib:Bosch1925_01">Iṁ su tuhān</rdg>
        </app>
        <app loc="1v6">
          <lem><gap quantity="1" reason="lost" unit="character"></gap>l· kalīma bakul· I satuhān· <gap quantity="2" reason="lost" unit="character"></gap> kapuA <gap quantity="5" reason="lost" unit="character"></gap> sasīma <gap quantity="3" reason="lost" unit="character"></gap></lem>
          <note>This whole line is omitted from <ptr target="bib:Boechari1985-1986_01"></ptr></note>
        </app>
        <app loc="1v6">
          <lem>kalīma bakul·</lem>
          <rdg source="bib:Bosch1925_01"><gap quantity="1" reason="lost" unit="character"></gap> līma bakal·</rdg>
        </app>
        <app loc="1v6">
          <lem>satuhān</lem>
          <rdg source="bib:Bosch1925_01"><gap quantity="1" reason="lost" unit="character"></gap> tuhan</rdg>
        </app>
