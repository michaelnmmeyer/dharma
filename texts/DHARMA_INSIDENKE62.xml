        </app>
        <app loc="1v7">
          <lem source="bib:Boechari1985-1986_01">salaḥha</lem>
          <rdg source="bib:Bosch1925_01">salaḥ <gap quantity="2" reason="lost" unit="character"></gap></rdg>
        </app>
