        <div n="4" type="textpart">
          <head xml:lang="eng">Face D</head>
          <lg met="anuṣṭubh" n="46">
            <l n="a" real="++++---+"><lb n="D1"></lb><g type="circleTarget">.</g> tatredaṁ vāsigaṇake <space></space></l>
            <l n="b">varṣe varṣe prakalpita<unclear>m·</unclear></l>
            <l n="c"><lb n="D2"></lb>saptaviṁśati gobhikṣā <space></space></l>
            <l n="d">Ekaśo 'rddhatrivimvikā</l>
          </lg>
          <lg met="anuṣṭubh" n="47">
            <l n="a"><lb n="D3"></lb><g type="circleTarget">.</g> vr̥hadvr̥<unclear>ha</unclear>tikās tisro <space></space></l>
            <l n="b">yugalāni tu vāsasām<unclear>·</unclear></l>
            <l n="c"><lb n="D4"></lb>pañcāśad daśa<unclear>has</unclear>tānaṁ <space></space></l>
            <l n="d">sārddhāni parisaṁkhyayā</l>
          </lg>
          <lg met="anuṣṭubh" n="48">
            <l n="a" real="++++---+"><lb n="D5"></lb><g type="circleTarget">.</g> praty-ekan <choice>
                <orig>vd</orig>
                <corr>dv</corr>
              </choice>ā-daśa-pale <space></space></l>
            <l n="b">takka-kr̥ṣṇe prakalpite</l>
            <l n="c"><lb n="D6"></lb>catasraḥ kaṭṭikāḥ pātra<space></space></l>
            <l n="d">trayasya trāpuṣasya tu</l>
          </lg>
          <lg met="anuṣṭubh" n="49">
            <l n="a"><lb n="D7"></lb><g type="circleTarget">.</g> Eka-viṁśati-khāryyas tu <space></space></l>
            <l n="b">ta<choice>
                <orig>nd</orig>
                <corr>ṇḍ</corr>
              </choice>ulāḥ parikalpitāḥ</l>
            <l enjamb="yes" n="c" real="++++---+"><lb n="D8"></lb>praty-avdaṁ vāsiṣu catu<space></space></l>
            <l n="d">rṣv idan dātavyam ekaśaḥ</l>
          </lg>
          <lg met="anuṣṭubh" n="50">
            <l n="a"><lb n="D9"></lb><unclear>
                <g type="circleTarget">.</g>
              </unclear> vāsasān daśa<unclear>ha</unclear>stānaṁ <space></space></l>
            <l n="b">yugmāni daśapañca ca</l>
            <l n="c" real="++-++--+"><lb n="D10"></lb>tisr<unclear>o</unclear> vr̥hatyas tripale <space></space></l>
