            <note>Sircar notes: “<quote>the lost <hi rend="italic">aksharas</hi> are no doubt -<hi rend="italic">varasa</hi>.</quote>”</note>
          </app>
          <app loc="2">
            <lem><gap quantity="1" reason="illegible" unit="character"/><unclear>raka</unclear>-</lem>
            <rdg source="bib:Sircara"><unclear>°arakama</unclear>-</rdg>
            <note>Sircar notes: “<quote>Better read <hi rend="italic">Arakasa</hi> separately from
								the compound.</quote>”</note>
          </app>
          <app loc="3">
            <lem>chāy<unclear>ekha</unclear>bha</lem>
            <rdg source="bib:Sircara">chayakhabho</rdg>
          </app>
        </listApp>
