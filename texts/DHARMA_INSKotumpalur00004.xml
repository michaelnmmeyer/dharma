<div type="apparatus">
             
<listApp>
                
                 
<app loc="1">
                     
<lem>
  <hi rend="grantha">s
  <unclear reason="eccentric_ductus">va</unclear>
  <supplied reason="omitted">sti</supplied></hi>
</lem>
                     
<note>The letter 
<foreign>va</foreign> looks like 
<foreign>pa</foreign>.</note>                     
