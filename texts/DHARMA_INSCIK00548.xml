   <lb n="1"/><gap extent="unknown" reason="lost" unit="character"/> qnak<supplied reason="lost">·</supplied> sre <g type="dotMid">.</g> Amraḥ va <g type="dotMid">.</g> va dhmak<supplied reason="lost">·</supplied> <gap extent="unknown" reason="lost" unit="character"/>
   <lb n="2"/><gap extent="unknown" reason="lost" unit="character"/> va kdāy· <g type="dotMid">.</g> va saUy· <g type="dotMid">.</g> va kuñ· <g type="dotMid">.</g> va <gap extent="unknown" reason="lost" unit="character"/>
   <lb n="3"/><gap extent="unknown" reason="lost" unit="character"/> va psaiy· <g type="dotMid">.</g> va kandās· <g type="dotMid">.</g> kantai ku <gap extent="unknown" reason="lost" unit="character"/>
   <lb n="4"/><gap extent="unknown" reason="lost" unit="character"/>p· <g type="dotMid">.</g> ku kloñ· <g type="dotMid">.</g> ku raṁ <gap extent="unknown" reason="lost" unit="character"/>
   <lb n="5"/><gap extent="unknown" reason="lost" unit="character"/> <num value="50">40 10</num> <g type="dotMid">.</g> krapi <num cert="low" value="22">20 <unclear>II</unclear></num> <gap extent="unknown" reason="lost" unit="character"/>
   <lb n="6"/><gap extent="unknown" reason="lost" unit="character"/>l· kanlaḥ sanre <gap extent="unknown" reason="lost" unit="character"/>
