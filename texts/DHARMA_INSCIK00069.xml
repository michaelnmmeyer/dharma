      <div type="edition" xml:lang="san-Latn">
        <div n="1" type="textpart">
          <head xml:lang="eng">Face</head>
          <lg met="sragdharā" n="1">
            <l n="a"><lb n="1"></lb><gap extent="unknown" reason="lost" unit="character"></gap>sonn<gap extent="unknown" reason="lost" unit="character"></gap></l>
            <l n="b">n n<supplied reason="lost">i</supplied>ghnātmanye <seg met="-++">
                <gap quantity="3" reason="lost" unit="character"></gap>
              </seg>nn<seg met="----">
                <gap quantity="4" reason="lost" unit="character"></gap>
              </seg>hr̥dayamān<seg subtype="vowel" type="component">
                <gap quantity="1" reason="lost" unit="component"></gap>
              </seg>v<seg met="++-++">
                <gap quantity="5" reason="lost" unit="character"></gap>
              </seg></l>
            <l n="c"><lb n="2"></lb><seg met="++++-++------++-+">
                <gap quantity="17" reason="lost" unit="character"></gap>
              </seg>sānavadyā</l>
            <l n="d">manye tābhis sakhībhis sma hasati nitarāṁ rāja-lakṣmīḥ kr̥terṣyā</l>
          </lg>
          <lg met="śārdūlavikrīḍita" n="2">
            <l n="a"><lb n="3"></lb><seg met="+++--+-+---+++-">
                <gap quantity="15" reason="lost" unit="character"></gap>
              </seg>r<seg subtype="body" type="component">
                <gap quantity="1" reason="lost" unit="component"></gap>
              </seg>āvandu<seg met="+">
                <gap quantity="1" reason="lost" unit="character"></gap>
              </seg></l>
            <l n="b">bhū-bhr̥n-nātha-mahīyasāṁ stuti-gatas santānakānān dhuri</l>
            <l n="c"><lb n="4"></lb><seg met="+++--+-+---+++-++-">
                <gap quantity="18" reason="lost" unit="character"></gap>
              </seg>lair</l>
            <l n="d">nityan nandiyitā tathāpi kathito yo pārijāto 'rthataḥ</l>
          </lg>
          <lg met="śārdūlavikrīḍita" n="3">
            <l n="a">
              <lb n="5"></lb>
              <seg met="+++--+-+---+++-++-+">
                <gap quantity="19" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="b">vedyan dūragatañ jijīviṣu vaneṣv ālīnām adriṣv api</l>
            <l n="c"><lb n="6"></lb><seg met="+++--+-+---+++-+">
                <gap quantity="16" reason="lost" unit="character"></gap>
              </seg>mantribhir</l>
            <l n="d">dhamnāñ ca dvija-rāja-saṅgatir abhūd bhūmnā kakuvvyāpinām</l>
          </lg>
          <lg met="sragdharā" n="4">
            <l n="a"><lb n="7"></lb><seg met="++++-++------++-++-+">
                <gap quantity="20" reason="lost" unit="character"></gap>
              </seg>ṣṭau</l>
            <l n="b">bhītyā śrīsūryyavarmmābhidhi-nihita-hr̥dāṁ prītaye pri piyoktam</l>
            <l n="c"><lb n="8"></lb><seg met="++++-++------++-+">
                <gap quantity="17" reason="lost" unit="character"></gap>
              </seg>vartma gacche</l>
            <l n="d"><seg met="++">
                <gap quantity="2" reason="lost" unit="character"></gap>
              </seg>tan<seg met="+-+">
                <gap quantity="3" reason="lost" unit="character"></gap>
              </seg>saṁ smr̥tim apahr̥tavad vidviṣāṁ yasya maṇḍe</l>
          </lg>
          <lg met="śārdūlavikrīḍita" n="5">
            <l n="a"><lb n="9"></lb><seg met="+++--+-+---+++-+">
                <gap quantity="16" reason="lost" unit="character"></gap>
              </seg>o vinā</l>
            <l n="b"><seg met="++">
                <gap quantity="2" reason="lost" unit="character"></gap>
              </seg>rbhā<seg met="--+">
                <gap quantity="3" reason="lost" unit="character"></gap>
              </seg>dvijadhvanivaraṁ saṁvibhratī bhrājitā</l>
            <l n="c">
              <lb n="10"></lb>
              <seg met="+++--+-+---+++-++-+">
                <gap quantity="19" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="d"><seg met="+++--+">
                <gap quantity="4" reason="lost" unit="character"></gap>
              </seg>vārīkr̥tavatā yeneva saṁrakṣitā</l>
          </lg>
          <lg met="sragdharā" n="6">
            <l n="a"><lb n="11"></lb><seg met="++++-++------++-">
                <gap quantity="16" reason="lost" unit="character"></gap>
              </seg>rociṣpra<seg met="+">
                <gap quantity="4" reason="lost" unit="character"></gap>
              </seg>nor</l>
            <l n="b">nītapr<seg subtype="vowel" type="component">
                <gap quantity="1" reason="lost" unit="component"></gap>
              </seg><seg met="+-+">
                <gap quantity="3" reason="lost" unit="character"></gap>
              </seg>ccair mmukharitakuharaṅ-kīrtti-bhr̥ṅgīni-nādaiḥ</l>
            <l n="c"><lb n="12"></lb><seg met="++++-++------++-++">
                <gap quantity="18" reason="lost" unit="character"></gap>
              </seg>kṣa<seg met="+">
                <gap quantity="1" reason="lost" unit="character"></gap>
              </seg>yo</l>
            <l n="d">manye <seg met="++-+">
                <gap quantity="4" reason="lost" unit="character"></gap>
              </seg>mo madhuvidhuticalan nābhipadmaṁ murārim</l>
          </lg>
          <lg met="sragdharā" n="7">
            <l n="a"><lb n="13"></lb><seg met="++++-++------+">
                <gap quantity="14" reason="lost" unit="character"></gap>
              </seg>mārggasa<seg met="-+">
                <gap quantity="2" reason="lost" unit="character"></gap>
              </seg>cārvyaḥ</l>
            <l n="b"><seg met="++++">
                <gap quantity="4" reason="lost" unit="character"></gap>
              </seg>ṅgamātre kr̥ta-kr̥ta-karuṣā dūrato bhāyayantyaḥ</l>
            <l n="c"><lb n="14"></lb><seg met="++++-++------++-++-">
                <gap quantity="19" reason="lost" unit="character"></gap>
              </seg>sattyā</l>
            <l n="d"><seg met="++++-">
                <gap quantity="4" reason="lost" unit="character"></gap>
              </seg>tanvan sakalajanamude leśato yasya līlām</l>
          </lg>
          <lg met="śārdūlavikrīḍita" n="8">
            <l n="a">
              <lb n="15"></lb>
              <seg met="+++--+-+---+++-++-+">
                <gap quantity="19" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="b"><seg met="+++--">
                <gap quantity="5" reason="lost" unit="character"></gap>
              </seg>ti kṣamānuvasatiṁ sādhvī kriyā krātavī</l>
            <l n="c">
              <lb n="16"></lb>
              <seg met="+++--+-+---+++-++-+">
                <gap quantity="19" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="d"><seg met="+++-">
                <gap quantity="4" reason="lost" unit="character"></gap>
              </seg><seg subtype="body" type="component">
                <gap quantity="1" reason="lost" unit="component"></gap>
              </seg>i yatra sādhukathitā saty apy anītiḥ prajā</l>
          </lg>
          <lg met="sragdharā" n="9">
            <l n="a">
              <lb n="17"></lb>
              <seg met="++++-++------++-++-++">
                <gap quantity="21" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="b"><seg met="++++-">
                <gap quantity="5" reason="lost" unit="character"></gap>
              </seg>karttā kumuda-vana-vibhādhāyi rociṣpracāraḥ</l>
            <l n="c">
              <lb n="18"></lb>
              <seg met="++++-++------++-++-++">
                <gap quantity="21" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="d"><seg met="++++">
                <gap quantity="4" reason="lost" unit="character"></gap>
              </seg>samudyan sma vahati nitarāṁ līlayā rājaśailīm</l>
          </lg>
          <lg met="śārdūlavikrīḍita" n="10">
            <l n="a">
              <lb n="19"></lb>
              <seg met="+++--+-+---+++-++-+">
                <gap quantity="19" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="b"><seg met="+++-">
                <gap quantity="4" reason="lost" unit="character"></gap>
              </seg>d<seg subtype="vowel" type="component">
                <gap quantity="1" reason="lost" unit="component"></gap>
              </seg><seg met="+">
                <gap quantity="1" reason="lost" unit="character"></gap>
              </seg>mmu<seg met="+---">
                <gap quantity="4" reason="lost" unit="character"></gap>
              </seg>laḥ kīrttidhvanau ghūrṇṇite</l>
            <l n="c">
              <lb n="20"></lb>
              <seg met="+++--+-+---+++-++-+">
                <gap quantity="19" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="d"><seg met="+++--+-+---+">
                <gap quantity="12" reason="lost" unit="character"></gap>
              </seg>t kṣoṇyāṁ sphuṭan tiṣṭhataḥ</l>
          </lg>
          <lg met="śārdūlavikrīḍita" n="11">
            <l n="a">
              <lb n="21"></lb>
              <seg met="+++--+-+---+++-++-+">
                <gap quantity="19" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="b"><seg met="+++--+-+---++">
                <gap quantity="13" reason="lost" unit="character"></gap>
              </seg>ndro jadimnānvitaḥ</l>
            <l n="c">
              <lb n="22"></lb>
              <seg met="+++--+-+---+++-++-+">
                <gap quantity="19" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="d"><seg met="+++--+-+---++">
                <gap quantity="13" reason="lost" unit="character"></gap>
              </seg>yān paṭimnā ruceḥ</l>
          </lg>
          <lg met="sragdharā" n="12">
            <l n="a">
              <lb n="23"></lb>
              <seg met="++++-++------++-++-++">
                <gap quantity="21" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="b"><seg met="+++--+-+---+++">
                <gap quantity="14" reason="lost" unit="character"></gap>
              </seg>karo 'py amvudhiḥ</l>
            <l n="c">
              <lb n="24"></lb>
              <seg met="++++-++------++-++-++">
                <gap quantity="21" reason="lost" unit="character"></gap>
              </seg>
            </l>
            <l n="d"><gap extent="unknown" reason="lost" unit="character"></gap>mya<gap extent="unknown" reason="lost" unit="character"></gap></l>
          </lg>
