  </desc><classes><memberOf key="att.global"/><memberOf key="model.pPart.transcriptional"/><memberOf key="att.transcriptional"/><memberOf key="att.placement"/><memberOf key="att.typed"/><memberOf key="att.dimensions"/></classes><content><macroRef key="macro.paraContent"/></content><attList><attDef ident="place" mode="change" usage="req">
