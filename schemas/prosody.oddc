          <gloss versionDate="2007-11-06" xml:lang="it">analitico</gloss>
          <gloss versionDate="2007-05-04" xml:lang="es">analítico</gloss>
          <gloss versionDate="2017-06-04" xml:lang="de">unselbständig</gloss>
