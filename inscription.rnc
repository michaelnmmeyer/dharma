ns1.surname = element ns1:surname { text }
ns1.fw =
  element ns1:fw {
    attribute n { xsd:NMTOKEN }?,
    attribute place { xsd:NCName },
    attribute rend { xsd:NCName }?,
    (text | ns1.g | ns1.gap | ns1.num | ns1.supplied | ns1.unclear)+
  }
ns1.expan =
  element ns1:expan {
    ns1.abbr,
    element ns1:ex { xsd:NCName }
  }
ns1.sic =
  element ns1:sic {
    (text
     | ns1.add
     | ns1.choice
     | ns1.corr
     | ns1.foreign
     | ns1.g
     | ns1.hi
     | ns1.lb
     | ns1.num
     | ns1.orig
     | ns1.pb
     | ns1.seg
     | ns1.space
     | ns1.subst
     | ns1.supplied
     | ns1.surplus
     | ns1.unclear)+
  }
ns1.cit = element ns1:cit { ns1.q | (ns1.bibl | ns1.quote)+ }
ns1.rs =
  element ns1:rs {
    attribute ana { xsd:NCName }?,
    attribute key { xsd:NCName }?,
    attribute n { xsd:integer }?,
    attribute nymRef { xsd:NCName }?,
    attribute ref { xsd:NMTOKEN }?,
    attribute role { xsd:NCName }?,
    attribute subtype { xsd:NCName }?,
    attribute type { xsd:NCName }?,
    (text
     | ns1.choice
     | ns1.lb
     | ns1.persName
     | ns1.roleName
     | ns1.rs
     | ns1.supplied
     | ns1.surplus
     | ns1.unclear)+
  }
ns1.damage = element ns1:damage { (text | ns1.supplied | ns1.unclear)+ }
ns1.placeName =
  element ns1:placeName {
    attribute key { text }?,
    attribute next { text }?,
    attribute prev { text }?,
    attribute ref { xsd:NMTOKEN }?,
    attribute subtype { xsd:NCName }?,
