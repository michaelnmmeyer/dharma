persName =
  
  ## (personal name) contains a proper noun or proper-noun phrase referring to a person, possibly including one or more of the person's forenames, surnames, honorifics, added names, etc. [13.2.1. Personal Names]
  element persName {
    macro.phraseSeq,
    att.global.attribute.xmlid,
    att.canonical.attribute.key,
    
    ## (reference) provides an explicit means of locating a full definition or identity for the entity being named by means of one or more URIs.
    attribute ref {
      xsd:token { pattern = "part:[a-z]{4}" }
      | xsd:anyURI { pattern = "http.+" }
    }?,
    
    ## characterizes the element in some sense, using any convenient classification scheme or typology.
    ## Sample values include: 1] divine; 2] human; 3] personification
    attribute type {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    
    ## provides a sub-categorization of the element, if needed
    ## Sample values include: 1] coronation; 2] sobriquet; 3] title; 4] other
    attribute subtype {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }
