num =
  
  ## (number) contains a number, written in any form. [3.5.3. Numbers and
  ## Measures]
  element num {
    macro.phraseSeq
    >> sch:pattern [
         id = "dharma-num-valeur-or-interval-constraint-rule-31"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:num"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@value and (@atMost or @atLeast)"
             "@value can not be used\x{a}" ~
             "                                    with the attributes of date range"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test = "@atMost and not(@atLeast)"
             "@atLeast is expected with\x{a}" ~
             "                                    @atMost"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test = "@atLeast and not(@atMost)"
             "@atMost is expected with\x{a}" ~
             "                            @atLeast"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    att.global.responsibility.attribute.cert,
    att.ranging.attribute.atLeast,
    att.ranging.attribute.atMost,
    
    ## supplies the value of the number in standard form.
    attribute value {
      xsd:double
      | xsd:token { pattern = "(\-?[\d]+/\-?[\d]+)" }
      | xsd:decimal
    }?,
    empty
  }
measure =
  
  ## contains a word or phrase referring to some quantity of an object or commodity, usually comprising a number, a unit, and a commodity name. [3.5.3. Numbers and
  ## Measures]
  element measure {
    macro.phraseSeq,
    att.measurement.attribute.unit,
    att.measurement.attribute.quantity,
    att.measurement.attribute.commodity,
    
    ## specifies the type of measurement in any convenient typology.
    attribute type {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }
