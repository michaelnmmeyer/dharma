space =
  
  ## indicates the location of a significant space in the text. [11.5.1. Space]
  element space {
    empty
    >> sch:pattern [
         id = "dharma-space-space-constraint-rule-50"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:space"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@quantity and not(@unit)"
             "If space has @quantity then\x{a}" ~
             "                                    @unit is required"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:space"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "@type='binding-hole' or @type='descender' or @type='ascender' or @type='defect' or @type='feature' and @quantity and @unit"
             "Space can't have @quantity and @unit attributes with\x{a}" ~
             "                                    @type='binding-hole|descender|ascender|defect|feature'."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    
    ## characterizes the element in some sense, using any convenient classification scheme or typology.
    attribute type {
      
      ## area that was left blank when the rest of the inscription was engraved, possibly with the intent to be filled later on
      "vacat"
      | 
        ## if the binding hole in a copper plate affects the text of a line
        "binding-hole"
      | 
        ## if a space was left blank in a line because (part of) another character hanging down from the previous line encroaches on the current line
        "descender"
      | 
        ## if a space was left blank in a line because (part of) another (pre-conceived) character popping up from the following line encroaches on the current line
        "ascender"
      | 
        ## if the writing skips a blemish of the surface that was not deliberately created (such as a natural crack or pit, or a fault in the creation of the writing surface)
        "defect"
      | 
        ## if the writing skips a deliberately created feature (other than binding holes, ascenders and descenders covered above) on the surface (such as engraved artwork, high relief, or a seal attached directly to a copper plate)
        "feature"
      | 
        ## significant space that does not fit the other categories
        "unclassified"
    }?,
    
    ## names the unit used for the measurement
    [ a:defaultValue = "character" ]
    attribute unit {
      
      ##
      "character"
    }?,
    
    ## specifies the length in the units specified
    attribute quantity { xsd:positiveInteger }?,
    empty
  }
