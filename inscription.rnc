gap =
  
  ## indicates a point where material has been omitted in a transcription, whether for editorial reasons described in the TEI header, as part of sampling practice, or because the material is illegible, invisible, or inaudible. [3.4.3. Additions, Deletions, and Omissions]
  element gap {
    empty
    >> sch:pattern [
         id = "dharma-gap-gap-constraint-rule-13"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:gap"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@quantity and @extent"
             "gap may have @quantity (a figure)\x{a}" ~
             "                                    or @extent (a descriptive text value) but not\x{a}" ~
             "                                    both"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test = "@quantity and not(@unit)"
             "If\x{a}" ~
             "                                    gap has @quantity then @unit is required"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "not(@reason='ellipsis') and ancestor::tei:supplied[not(@reason='undefined')]"
             "gap may not appear within supplied text"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test = "@precision and not(@quantity)"
             "@precision can be use only\x{a}" ~
             "                                    with @quantity"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "ancestor::tei:div[@type='aparatus'] and not(@reason)"
             "Only\x{a}" ~
             "                                    @reason is mandatory in\x{a}" ~
             "                                div[@type='translation']"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:gap[@reason='ellipsis']"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "not(ancestor::tei:div[@type='apparatus'] or ancestor::tei:div[@type='translation'])"
             " The element gap with @reason='ellipsis' can only be used\x{a}" ~
             "                                    inside the apparatus or in\x{a}" ~
             "                                translation."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:gap[@unit='component']"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "parent::tei:seg[@type='component']"
             "Gap with the attribute\x{a}" ~
             '                                    @unit="component" can only be used inside seg with the\x{a}' ~
             '                                    @type="component". '
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:gap[@unit='line' and @quantity] "
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@precision='low' or child::tei:certainty"
             "The element gap\x{a}" ~
             '                                    with @quantity and @unit="line" expect either the use of\x{a}' ~
             '                                    @precision="low" or the element\x{a}' ~
             "                                certainty."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    
    ##
    attribute precision {
      
      ##
      "low"
    }?,
    
    ## names the unit used for the measurement
    attribute unit {
      
      ##
      "character"
      | 
        ##
        "component"
      | 
        ##
        "line"
      | 
        ##
        "page"
      | 
        ##
        "plate"
      | 
        ##
        "folio"
    }?,
    
    ## specifies the length in the units specified
    attribute quantity { xsd:positiveInteger }?,
    
    ## indicates the size of the object concerned using a project-specific vocabulary combining quantity and units in a single string of words.
    attribute extent {
      
      ##
      "unknown"
    }?,
    
    ## gives the reason for omission
    attribute reason {
      
      ##
      "lost"
      | 
        ##
        "illegible"
      | 
        ##
        "omitted"
      | 
        ##
        "ellipsis"
      | 
        ##
        "undefined"
    },
    empty
  }
add =
  
  ## (addition) contains letters, words, or phrases inserted in the source text by an author, scribe, or a previous annotator or corrector. [EGD 4.4.2. Premodern insertion EGC 5.5.13.1. <add> – Additions]
  element add {
    macro.paraContent,
    
    ## (rendition) indicates how the element in question was rendered or presented in the source text.
    attribute rend {
      
      ## when the addition is marked in the witness with any non-alphanumeric mark (such as the cross-shaped called kākapada in Sanskrit) 
      "mark"
    }?,
    
    ## specifies where this item is placed.
    [ a:defaultValue = "unspecified" ]
    attribute place {
      
      ## addition made within the original text (in textu) 
      "inline"
      | 
        ## addition below the line (subscr.) 
        "below"
      | 
        ## addition above the line (suprascr.) 
        "above"
      | 
        ## addition in the top margin (in mg. sup. = in margine superiore) 
        "top"
      | 
        ## addition in the bottom margin (in mg. inf. = in margine inferiore) 
        "bottom"
      | 
        ## addition in the left margin (in mg. = in margine) 
        "left"
      | 
        ## addition in the right margin (in mg. = in margine) 
        "right"
      | 
        ## addition made in the space where a previous string of text has been erased (in ras. = in rasura) 
        "overstrike"
      | 
        ## when no information on the place is available 
        "unspecified"
    },
    empty
  }
del =
  
  ## (deletion) contains a letter, word, or passage deleted, marked as deleted, or otherwise indicated as superfluous or spurious in the copy text by an author, scribe, or a previous annotator or corrector. [3.4.3. Additions, Deletions, and Omissions]
  element del {
    macro.paraContent
    >> sch:pattern [
         id = "dharma-del-del-in-subst-constraint-rule-17"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:del"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@rend='corrected' and not(parent::tei:subst)"
             "@rend='corrected' is only allowed when del is a child of subst"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    
    ## (rendition) indicates how the element in question was rendered or presented in the source text.
    attribute rend {
      
      ## text struck through or cross-hatched
      "strikeout"
      | 
        ## combined application of vowel markers u and i to characters to be deleted
        "ui"
      | 
        ## deletion marker other than strikeout and ui
        "other"
      | 
        ## the text to be replaced was neither erased, nor marked for cancellation (i.e. it is either overwritten with the post-correction text or left in place without any apparent alteration)
        "corrected"
    }?,
    empty
  }
