settlement =
  
  ## contains the name of a settlement such as a city, town, or village identified as a single geo-political or administrative unit. [13.2.3. Place Names]
  element settlement { macro.phraseSeq }
model.rdgPart = notAllowed
app =
  
  ## (apparatus entry) contains one entry in a critical apparatus, with an optional lemma and usually one or more readings or notes on the relevant passage. [12.1.1. The Apparatus Entry]
  element app {
    (lem, rdg*, note*)
    >> sch:pattern [
         id = "dharma-app-one-lem-constraint-rule-64"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:app"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "count(child::tei:lem) eq 1 and not(child::tei:lem[@source]) and not(child::tei:rdg or child::tei:note)"
             "When the apparatus entry contains only a lem, the @source is\x{a}" ~
             "                                    mandatory"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    
    ## (location) indicates the location of the variation, when the location-referenced method of apparatus markup is used.
    attribute loc { xsd:string },
    empty
  }
listApp =
  
  ## (list of apparatus entries) contains a list of apparatus entries. [12.2. Linking the Apparatus to the Text]
  element listApp {
    (app+)
    >> sch:pattern [
         id = "dharma-listApp-div-constraint-rule-65"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:listApp"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "ancestor::tei:div[@type='apparatus']"
             "ListApp can only be\x{a}" ~
             "                                    used inside the apparatus."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
  }
lem =
  
  ## (lemma) contains the lemma, or base text, of a textual variation. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element lem {
    ((text
      | model.divLike
      | model.divPart
      | model.gLike
      | model.phrase
      | model.inter
      | model.global
      | model.rdgPart)*)
    >> sch:pattern [
         id = "dharma-lem-exclusion-inside-lem-constraint-rule-66"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:lem"
           "\x{a}" ~
           "               "
           sch:report [
             test = "child::tei:p"
             "The\x{a}" ~
             "                                    element p is not allowed in lem"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test = "child::tei:ab"
             "The element ab is not allowed in\x{a}" ~
             "                                    lem"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test = "child::tei:lg"
             "The element lg\x{a}" ~
             "                                    is not allowed in lem"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test = "count(child::tei:l) eq 1"
             "Don't keep the l element in lem\x{a}" ~
             "                                    if you have only one of\x{a}" ~
             "                            them"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    att.global.source.attribute.source,
    empty
  }
rdg =
  
  ## (reading) contains a single reading within a textual variation. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element rdg {
    ((text
      | model.divLike
      | model.divPart
      | model.gLike
      | model.phrase
      | model.inter
      | model.global
      | model.rdgPart)*)
    >> sch:pattern [
         id = "dharma-rdg-exclusion-inside-lem-constraint-rule-67"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:rdg"
           "\x{a}" ~
           "               "
           sch:report [
             test = "child::tei:p"
             "The\x{a}" ~
             "                                    element p is not allowed in rdg"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test = "child::tei:ab"
             "The element ab is not allowed in\x{a}" ~
             "                                    rdg"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test = "child::tei:lg"
             "The element lg\x{a}" ~
             "                                    is not allowed in rdg"
           ]
           "\x{a}" ~
           "               "
           sch:report [
             test = "child::tei:l"
             "The element l is not allowed in\x{a}" ~
             "                                    rdg"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    
    ## specifies the source from which some aspect of this element is drawn.
    attribute source {
      list { xsd:anyURI+ }
    },
    empty
  }
